<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import org.kordamp.ikonli.javafx.FontIcon?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.CartController">
   <children>
      <Label alignment="CENTER" contentDisplay="CENTER" layoutX="498.0" layoutY="24.0" text="Cart">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <ListView fx:id="cartList" layoutY="73.0" prefHeight="258.0" prefWidth="600.0" />
      <Label layoutX="14.0" layoutY="14.0" onMouseClicked="#goBack" text="←">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
       <FontIcon iconLiteral="fa-window-close" iconSize="20" layoutX="572.0" layoutY="21.0" onMouseClicked="#close" wrappingWidth="19.2000732421875" />
      <Button fx:id="checkOutBtn" layoutX="264.0" layoutY="361.0" mnemonicParsing="false" onAction="#checkOut" text="Check Out" />
      <Label fx:id="bill" layoutX="230.0" layoutY="50.0" prefHeight="18.0" prefWidth="140.0" />
      <RadioButton fx:id="debitbtn" layoutX="97.0" layoutY="343.0" mnemonicParsing="false" onAction="#payDebit" text="Pay With Debit" />
      <RadioButton fx:id="creditbtn" layoutX="410.0" layoutY="343.0" mnemonicParsing="false" onAction="#payCredit" text="Pay With Credit" />

   </children>
</AnchorPane>
